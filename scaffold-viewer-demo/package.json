{
  "name": "scaffold-viewer-demo",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/alan-wu/hubmap-ABIscaffolds"
  },
  "scripts": {
    "serve": "vue-cli-service serve --port 8081",
    "lint": "vue-cli-service lint --ext .js,.vue src",
    "build-bundle": "vue-cli-service build --target lib --name scaffoldvuer ./src/components/index.js",
    "build": "vue-cli-service build --dest dist"
  },
  "main": "hubmaps-abiscaffold",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "@abi-software/scaffoldvuer": "^0.1.53-beta.2",
    "vue": "^2.6.10",
    "vue-router": "^3.5.1",
    "zincjs": "^1.0.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-service": "^4.5.13",
    "babel-eslint": "^10.0.3",
    "babel-plugin-component": "^1.1.1",
    "base64-inline-loader": "^1.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.14.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^8.0.2",
    "simple-dropzone": "^0.8.1",
    "vue-cli-plugin-styleguidist": "^4.32.2",
    "vue-styleguidist": "^4.44.22",
    "vue-template-compiler": "^2.6.10",
    "webpack-node-externals": "^2.5.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
